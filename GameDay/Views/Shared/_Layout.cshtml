<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lets Get A Game Going!</title>
    <link rel="stylesheet" type="text/css" href="~/public/main-2d5bb3e9c5.css"/>
    <link rel="stylesheet" type="text/css" href="~/Content/site.css" />
</head>
<body>
<div class="navbar navbar-fixed-top NavbarBackgroup">
    <div>
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink(Constant.Title, Constant.Controller.Index, Constant.Controller.Home, new {area = ""}, new {@class = "navbar-brand blackFont"})
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                @Html.ActionLink(Constant.ViewModels.Location, Constant.Controller.Index, Constant.Controller.Address, new {area = ""}, new {@class = "navbar-brand blackFont"})
                @*<li>@Html.ActionLink("Player", "Index", "Player")</li>*@
            </ul>
            <div style="margin-right: 1em;">
                @Html.Partial(Constant.Partial.LoginPartial)
            </div>
        </div>
    </div>
</div>
<div class="body-content">
    @RenderBody()
</div>
</body>
</html>
<script src="~/public/vendor-1b2fbe4541.js"></script>
<script src="~/signalr/hubs"></script>
<script src="~/Scripts/js/site.js"></script>

